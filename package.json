{
    "name": "arun",
    "description": "Minimal personal blog theme",
    "demo": "http://arun.gbjsolution.com",
    "version": "1.1.1",
    "engines": {
        "ghost": ">=4.0.0",
        "ghost-api": "v4"
    },
    "author": {
        "name": "GBJ solution",
        "email": "gbjsolution@gmail.com",
        "url": "https://gbjsolution.com"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Personal Blogs",
            "Magazine Layouts"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme",
        "personal",
        "minimal"
    ],
    "config": {
        "posts_per_page": 8,
        "image_sizes": {
            "xxs": {
                "width": 150
            },
            "xs": {
                "width": 250
            },
            "s": {
                "width": 400
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 1200
            }
        }
    },
    "browserslist": [
        "defaults",
        "not IE 11"
    ],
    "devDependencies": {
        "autoprefixer": "^10.2.5",
        "browser-sync": "^2.26.14",
        "del": "^6.0.0",
        "gulp": "^4.0.2",
        "gulp-clean-css": "^4.3.0",
        "gulp-concat": "^2.6.1",
        "gulp-postcss": "^9.0.0",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^4.1.0",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-uglify": "^3.0.2",
        "gulp-wait": "0.0.2",
        "gulp-zip": "^5.1.0",
        "postcss": "^8.2.8"
    },
    "dependencies": {
        "clipboard": "^2.0.8",
        "disqusLoader.js": "git+https://github.com/osvaldasvalutis/disqusLoader.js.git",
        "medium-zoom": "^1.0.6",
        "prismjs": "^1.23.0",
        "reframe.js": "^3.0.2",
        "searchinghost": "^1.6.2"
    }
}
